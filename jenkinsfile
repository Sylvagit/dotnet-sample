pipeline{
    agent any
     
    environment{
        
        dotnet='C:\\Program Files\\dotnet'
    }

    stages
      {
        stage('checkout'){ 
            steps{
               git 'https://github.com/Sylvagit/production-ready-apis-sample-2.2.git'
            }
        }
      
         stage('Restore'){
             steps{
                bat 'dotnet restore C:\\ProgramData\\Jenkins\\.jenkins\\workspace\\demo-build\\BoardGamesApi\\BoardGamesApi.csproj'
                 
             }
         }
      }
}
