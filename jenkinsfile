pipeline{
    agent any
     
    environment{
        
        nuget = 'C:\\NuGet\\NuGet.exe'
        dotnet='C:\\Program Files\\dotnet'
    }

    stages
      {
        stage('checkout'){ 
            steps{
               git 'https://github.com/Sylvagit/dotnet-sample.git'
            }
        }
        stage('Restore'){
            steps{
                bat 'echo Restore'
                bat '%nuget% restore SampleWebApplication.sln'
            }
        }  
          
      }
      
}
         
